    <validationRules>
        <fullName>Validate_Market_ID</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK( Market_ID__c ),
   FALSE,  /* do not validate if empty */
   OR(
      LEN(Market_ID__c) &lt;&gt; 3,  /* must be 3 characters */
      NOT(ISNUMBER(Market_ID__c))  /* must be numeric */
     )
  )</errorConditionFormula>
        <errorDisplayField>Market_ID__c</errorDisplayField>
        <errorMessage>Market ID must be 3 digits</errorMessage>
    </validationRules>
