    <validationRules>
        <fullName>Promotion_For_Credit_Card_ACH</fullName>
        <active>true</active>
        <description>CSP-2898 Enable Discounts for a Promotional Period (Switch Pitch)</description>
        <errorConditionFormula>AND (
 ISCHANGED( Discount_Reason__c ),
 ISPICKVAL(Discount_Reason__c,&apos;Wedding Wire Switch Pitch&apos;),
 !ISPICKVAL(zqu__Quote__r.zqu__PaymentMethod__c,&apos;Credit Card&apos;),
 !ISPICKVAL(zqu__Quote__r.zqu__PaymentMethod__c,&apos;ACH&apos;) 
)</errorConditionFormula>
        <errorMessage>Only payment Method of Credit Card or ACH/EFT on quotes can select &apos;Free Trial 3 Mos&apos; as the discount reason!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotion_For_Discount</fullName>
        <active>true</active>
        <description>CSP-2898 Enable Discounts for a Promotional Period (Switch Pitch)</description>
        <errorConditionFormula>AND (
 ISCHANGED( Discount_Reason__c ),
 ISPICKVAL(Discount_Reason__c,&apos;Wedding Wire Switch Pitch&apos;),
 Discount_Override__c != 0  
)</errorConditionFormula>
        <errorMessage>Don&apos;t fill discount override if select &apos;Free Trial 3 Mos&apos; as the discount reason!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotion_For_Initial_Term_12</fullName>
        <active>true</active>
        <description>CSP-2898 Enable Discounts for a Promotional Period (Switch Pitch)</description>
        <errorConditionFormula>AND (
 ISCHANGED( Discount_Reason__c ),
 ISPICKVAL(Discount_Reason__c,&apos;Wedding Wire Switch Pitch&apos;),
 zqu__Quote__r.zqu__InitialTerm__c != 12 
)</errorConditionFormula>
        <errorMessage>Select &apos;Free Trial 3 Mos&apos; as the discount reason only apply Initial Term is 12!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotion_For_Prospect_Account</fullName>
        <active>true</active>
        <description>CSP-2898 Enable Discounts for a Promotional Period (Switch Pitch)</description>
        <errorConditionFormula>AND (
 ISCHANGED( Discount_Reason__c ),
 ISPICKVAL(Discount_Reason__c,&apos;Wedding Wire Switch Pitch&apos;),
 OR(
 	 AND(
 		 NOT(ISBLANK(zqu__Quote__r.zqu__Account__r.Promotional_Quote__c)), 
 		 zqu__Quote__r.zqu__Account__r.Promotional_Quote__c != zqu__Quote__c 
 	 ),
 	 AND(
 	 	ISBLANK(zqu__Quote__r.zqu__Account__r.Promotional_Quote__c),
 	 	NOT(ISBLANK(TEXT(zqu__Quote__r.zqu__Account__r.Account_Anniversary_Status__c))), 
	        NOT(ISPICKVAL(zqu__Quote__r.zqu__Account__r.Account_Anniversary_Status__c,&apos;Prospect&apos;))
 	 )
  )
)</errorConditionFormula>
        <errorMessage>Only Accounts with an Anniversary Status of Prospect can select &apos;Free Trial 3 Mos&apos; as the discount reason!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotion_For_Storefront_and_FV</fullName>
        <active>true</active>
        <description>CSP-2898 Enable Discounts for a Promotional Period (Switch Pitch)</description>
        <errorConditionFormula>AND (
 ISCHANGED( Discount_Reason__c ),
 ISPICKVAL(Discount_Reason__c,&apos;Wedding Wire Switch Pitch&apos;),
 !CONTAINS(zqu__QuoteProductName__c, &apos;Storefront&apos;),
 OR( CONTAINS(zqu__QuoteProductName__c, &apos;Premium&apos;),
     !CONTAINS(zqu__QuoteProductName__c, &apos;Featured Vendor&apos;)
 )   
)</errorConditionFormula>
        <errorMessage>Only storefront and featured vendor can select &apos;Free Trial 3 Mos&apos; as the discount reason!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_a_different_discount_reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   Discount_Override__c &gt; Steelbrick_Calculated_Discount__c,
   ISPICKVAL( Discount_Reason__c, &apos;None - Autocalculated Discount&apos;)
)</errorConditionFormula>
        <errorDisplayField>Discount_Reason__c</errorDisplayField>
        <errorMessage>When overriding the discount, you must choose a different reason</errorMessage>
    </validationRules>
</CustomObject>
