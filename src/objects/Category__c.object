    <validationRules>
        <fullName>Validate_Category_Code</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(Category_Code__c),
   FALSE,  /* do not validate if empty */
   LEN(Category_Code__c) &lt;&gt; 3  /* must be 3 characters */
  )</errorConditionFormula>
        <errorDisplayField>Category_Code__c</errorDisplayField>
        <errorMessage>Category Code must be 3 characters long</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Category_ID</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(Category_ID__c),
   FALSE,  /* do not validate if empty */
   OR(
      LEN(Category_ID__c) &lt;&gt; 3,  /* must be 3 characters */
      NOT(ISNUMBER(Category_ID__c))  /* must be numeric */
     )
  )</errorConditionFormula>
        <errorDisplayField>Category_ID__c</errorDisplayField>
        <errorMessage>Category ID must be 3 digits long</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Subcategory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Category_Group__c, &apos;SubCategory&apos;), ISBLANK( Parent_Category__c))</errorConditionFormula>
        <errorDisplayField>Parent_Category__c</errorDisplayField>
        <errorMessage>SubCategories require a Parent Category</errorMessage>
    </validationRules>
