    <validationRules>
        <fullName>Do_not_allow_0_dollar_TCV_charges</fullName>
        <active>false</active>
        <description>To reduce our data footprint and clear up confusion for sales reps, we are not syncing down the charges that have a $0 value.</description>
        <errorConditionFormula>AND(Zuora__TotalContractValue__c = 0,  
OR(Zuora__Subscription__r.Product_Line__c = &apos;&apos;,
   Zuora__Subscription__r.Product_Line__c = &apos;Migrated Contract&apos;),
Name &lt;&gt; &apos;Charge #00&apos;
)</errorConditionFormula>
        <errorDisplayField>Zuora__TotalContractValue__c</errorDisplayField>
        <errorMessage>$0 legacy charges are ignored when the Subscription has a TCV.  Only the initial charge (Charge #00) may be $0.</errorMessage>
    </validationRules>
