    <fields>
        <fullName>CommissionLineItemStatus__c</fullName>
        <description>A field to tag the subs line items status for 2019 Commission (CSP-3086)</description>
        <externalId>false</externalId>
        <formula>IF(OR(CONTAINS(Name,&apos;#&apos; ),Name=&apos;CES Charge Migrated&apos;),
  &apos;Migrated-Expired&apos;,
  IF(Zuora__Subscription__r.Zuora__TermStartDate__c&gt;Zuora__Subscription__r.Zuora__TermEndDate__c, 
    &apos;Invalid&apos;,
    IF(Zuora__Subscription__r.Product_Line__c=&apos;Internet&apos;,
      If(Zuora__EffectiveStartDate__c&gt;Today(), 
        &apos;Pending&apos;,
        If(Zuora__EffectiveEndDate__c&gt;=Today(), 
          &apos;Live&apos;,
          If(Zuora__Subscription__r.Zuora__Status__c=&apos;Cancelled&apos;, 
            &apos;Cancelled&apos;, 
            &apos;Expired&apos;
          )
        )
      ),  
      IF(Zuora__Subscription__r.Product_Line__c=&apos;Direct Mail&apos;,
        If( And(year(LDE_Email_Launch_Date__c)= year(today()),month(LDE_Email_Launch_Date__c)= month(today())), 
          &apos;Live&apos;,
          If( today() &lt; LDE_Email_Launch_Date__c, 
            &apos;Pending&apos;,
            If(Zuora__Subscription__r.Zuora__Status__c=&apos;Cancelled&apos;,
              &apos;Cancelled&apos;, 
              &apos;Expired&apos;
            )
          )
        ),
        IF(Zuora__Subscription__r.Product_Line__c=&apos;Print&apos;,
          If( And(year(X1st_cover_ship_date__c)= year(today()),month(X1st_cover_ship_date__c)= month(today())), 
            &apos;Live&apos;,
            If( today() &lt; X1st_cover_ship_date__c, 
              &apos;Pending&apos;,
              If(Zuora__Subscription__r.Zuora__Status__c=&apos;Cancelled&apos;, 
                &apos;Cancelled&apos;, 
                &apos;Expired&apos;)
            )
          )  
          ,&quot;Unknown&quot;
        )
      )
    )
  )
)</formula>
        <label>Commission Line Item Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
