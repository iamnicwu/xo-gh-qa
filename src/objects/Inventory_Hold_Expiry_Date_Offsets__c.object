<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>For setting the number of days to offset the inventory expiry date when dealing with different objects.  Negative numbers mean before the date in question, positive numbers mean after the date in question.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Account_Day_Offset__c</fullName>
        <defaultValue>3</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Number of work days from today&apos;s date to hold the inventory</inlineHelpText>
        <label>Account Day Offset</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Quote_Rate_Plan_Day_Offset__c</fullName>
        <defaultValue>0</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>The number of  work days before or after the Quote &quot;Valid Until&quot; date to hold the inventory</inlineHelpText>
        <label>Quote Rate Plan Day Offset</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Subscription_Product_Charge_Day_Offset__c</fullName>
        <defaultValue>-7</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>The number of work days before or after the subscription expire date to hold the inventory</inlineHelpText>
        <label>Subscription Product Charge Day Offset</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Inventory Hold Expiry Date Offsets</label>
    <visibility>Public</visibility>
</CustomObject>
