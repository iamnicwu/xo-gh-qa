        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/apex.js&apos;)}
var oppStage = &apos;{!Opportunity.StageName}&apos;; 
if(oppStage == &apos;Closed Won&apos;) {
alert(&apos;{!$Label.Opportunity_Closed_Won_Reopen_Error}&apos;); 
}
else if(oppStage == &apos;Closed Lost&apos;) {
alert(&apos;{!$Label.Opportunity_Closed_Lost_Reopen_Error}&apos;); 
}
else{
// test the merge field&apos;s value against the value that you expect
var oppLocked = &apos;{!Opportunity.Lock_Quotes__c}&apos; === &apos;1&apos;;
//var oppStage = &apos;{!Opportunity.StageName}&apos;; 
var oppContractSignerExist = &apos;{!Opportunity.Contract_Signer__c != null}&apos;;
var accountId = &apos;{!Opportunity.AccountId}&apos;;
var primaryContactRole = sforce.connection.query(&quot;SELECT Id FROM AccountContactRole WHERE AccountId = &apos;&quot; + accountId + &quot;&apos; AND (Role = &apos;Primary&apos; OR Role = &apos;Billing&apos;) LIMIT 1&quot;);
if (oppLocked || oppStage === &apos;Closed Won&apos; || oppStage === &apos;Closed Lost&apos;) {
alert(&apos;Opportunity is locked&apos;);
}
else if (oppContractSignerExist === &apos;false&apos;) {
alert(&apos;Please designate a Contract Signer when you are quoting, thanks!&apos;);
}
else if (primaryContactRole.size === &apos;0&apos;) {
   alert(&apos;Please add a Primary or Billing contact role in order to proceed with building your quote&apos;);
}
else {
// identify the record
var o = new sforce.SObject(&quot;Opportunity&quot;);
o.id = &quot;{!Opportunity.Id}&quot;;

// make the field change
o.StageName = &quot;Quoting&quot;;

// save the change
sforce.connection.update([o]);

window.location = &apos;/apex/zqu__QuoteOption?scontrolCaching=1&amp;id={!Opportunity.Id}&apos;;
}
}</url>
