        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/36.0/apex.js&apos;)}

/*
 The profiles that should have permission are:
 national business manager
 sales development
 vp, sales operations
 vp, national sales
 and system admin
*/
if(&apos;{!$UserRole.Name}&apos; == &apos;Business Manager&apos; ||
   &apos;{!$UserRole.Name}&apos; == &apos;Sales Development&apos; ||
   &apos;{!$UserRole.Name}&apos; == &apos;VP, National Sales&apos; ||
