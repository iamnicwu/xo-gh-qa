        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)}
var currentQuotes = sforce.connection.query(&quot;SELECT Name, zqu__Account__r.Name, SoldToContactQuoteTemplateMergeField__c, BillToContactQuoteTemplateMergeField__c, zqu__PaymentMethod__c, Preferred_Payment_Schedule__c, zqu__BillCycleDay__c, AutoRenewal__c FROM zqu__Quote__c WHERE zqu__Opportunity__c = &apos;{!Opportunity.Id}&apos;&quot;);

var bodyHtml = &apos;&lt;div style=&quot;text-align: center; padding: 20px;&quot;&gt;&apos;;
bodyHtml += &apos;&lt;span style=&quot;display: inline-block; font-size: 20; color: grey; margin: 0 0 20px 0;&quot;&gt;Service Summary Page&lt;/span&gt;&apos;;
bodyHtml += &apos;&lt;table border=&quot;1&quot; style=&quot;border-collapse: collapse; width: 100%;&quot;&gt;&apos;;
// Bill To/Sold To Row
bodyHtml += &apos;&lt;tr&gt;&apos;;
// Bill To
bodyHtml += &apos;&lt;td style=&quot;padding: 10px;&quot;&gt;&apos;;
bodyHtml += &apos;&lt;span style=&quot;font-weight: bold; font-size: 20;&quot;&gt;Bill To&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&apos;;
bodyHtml += &apos;{ACCOUNTNAME}&lt;br/&gt;&apos;;
bodyHtml += &apos;{BILLTOMERGE}&lt;br/&gt;&apos;;
bodyHtml += &apos;&lt;/td&gt;&apos;;
// Sold To
bodyHtml += &apos;&lt;td style=&quot;padding: 10px;&quot;&gt;&apos;;
bodyHtml += &apos;&lt;span style=&quot;font-weight: bold; font-size: 20;&quot;&gt;Sold To&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&apos;;
bodyHtml += &apos;{ACCOUNTNAME}&lt;br/&gt;&apos;;
bodyHtml += &apos;{SOLDTOMERGE}&lt;br/&gt;&apos;;
bodyHtml += &apos;&lt;/td&gt;&apos;;
bodyHtml += &apos;&lt;/tr&gt;&apos;;
// Payment Details
bodyHtml += &apos;&lt;tr&gt;&apos;;
bodyHtml += &apos;&lt;td colspan=&quot;2&quot; style=&quot;padding: 10px;&quot;&gt;&apos;;
bodyHtml += &apos;&lt;span style=&quot;font-weight: bold; font-size: 20;&quot;&gt;Payment Details&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&apos;;
bodyHtml += &apos;&lt;span style=&quot;font-weight: bold;&quot;&gt;Payment Method:&lt;/span&gt; {PAYMENTMETHOD}&lt;br/&gt;&apos;;
bodyHtml += &apos;&lt;span style=&quot;font-weight: bold;&quot;&gt;Pay Period:&lt;/span&gt; {PAYPERIOD}&lt;br/&gt;&apos;;
bodyHtml += &apos;&lt;span style=&quot;font-weight: bold;&quot;&gt;Bill Cycle Day:&lt;/span&gt; {BCDMERGE}&lt;br/&gt;&apos;;
bodyHtml += &apos;&lt;span style=&quot;font-weight: bold;&quot;&gt;Auto Renew:&lt;/span&gt; {AUTORENEWMERGE}&lt;br/&gt;&apos;;
bodyHtml += &apos;&lt;/td&gt;&apos;;
bodyHtml += &apos;&lt;/tr&gt;&apos;;
bodyHtml += &apos;&lt;/table&gt;&apos;;
bodyHtml += &apos;&lt;/div&gt;&apos;;

if(currentQuotes.records !== &apos;undefined&apos; || currentQuotes.size &gt; 0){
    for(var i = 0; i &lt; currentQuotes.size; i++) {
        var currentRecord;
        if(currentQuotes.size == 1) {
            currentRecord = currentQuotes.records;
        } else {
            currentRecord = currentQuotes.records[i];
        }
    console.log(currentRecord);  
    var printWindow = window.open(&apos;&apos;, &apos;&apos;, &apos;height=400,width=800&apos;);
    printWindow.document.write(&apos;&lt;html&gt;&lt;head&gt;&lt;title&gt;&apos; + currentRecord.Name + &apos;_preview&lt;/title&gt;&apos;);
    printWindow.document.write(&apos;&lt;style&gt;@media print {@page { margin: 0; }body { margin: 1.6cm; }}&lt;/style&gt;&apos;);
    printWindow.document.write(&apos;&lt;/head&gt;&lt;body &gt;&apos;);
    var bodyReplace = bodyHtml;
    bodyReplace = bodyReplace.replace(/{ACCOUNTNAME}/g, currentRecord.zqu__Account__r.Name);
    console.log(currentRecord.BillToContactQuoteTemplateMergeField__c);    
    if(currentRecord.BillToContactQuoteTemplateMergeField__c !== null) {
        bodyReplace = bodyReplace.replace(&apos;{BILLTOMERGE}&apos;, currentRecord.BillToContactQuoteTemplateMergeField__c.replace(/(?:\r\n|\r|\n)/g, &apos;&lt;br /&gt;&apos;));
    } else {
        bodyReplace = bodyReplace.replace(&apos;{BILLTOMERGE}&apos;, &apos;&apos;);
    }
    console.log(currentRecord.SoldToContactQuoteTemplateMergeField__c);
    if(currentRecord.SoldToContactQuoteTemplateMergeField__c !== null) {
        bodyReplace = bodyReplace.replace(&apos;{SOLDTOMERGE}&apos;, currentRecord.SoldToContactQuoteTemplateMergeField__c.replace(/(?:\r\n|\r|\n)/g, &apos;&lt;br /&gt;&apos;));
    } else {
        bodyReplace = bodyReplace.replace(&apos;{SOLDTOMERGE}&apos;, &apos;&apos;);
    }
    bodyReplace = bodyReplace.replace(&apos;{PAYMENTMETHOD}&apos;, currentRecord.zqu__PaymentMethod__c);
    bodyReplace = bodyReplace.replace(&apos;{PAYPERIOD}&apos;, currentRecord.Preferred_Payment_Schedule__c);
    bodyReplace = bodyReplace.replace(&apos;{BCDMERGE}&apos;, currentRecord.zqu__BillCycleDay__c);
    bodyReplace = bodyReplace.replace(&apos;{AUTORENEWMERGE}&apos;, currentRecord.AutoRenewal__c); 
    printWindow.document.write(bodyReplace);
    printWindow.document.write(&apos;&lt;/body&gt;&lt;/html&gt;&apos;);
    printWindow.document.close();
    printWindow.print();
    }
} else {
  alert(&apos;There are no quotes on this Opportunity&apos;);
}</url>
