        <masterLabel>Email with Tout</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/9.0/connection.js&quot;)} 

var queryResult = sforce.connection.queryAll(&quot;select c.Id, c.Name, c.Email, isPrimary, Role from OpportunityContactRole o, o.Contact c where OpportunityId = &apos;{!Opportunity.Id}&apos;&quot;);
var records = queryResult.getArray(&quot;records&quot;);

var contactId = 0;
var contactName = &quot;&quot;;
var contactEmail = &quot;&quot;;
var cc = [];

for (var i=0; i&lt;records.length; i++) {
  var record = records[i];
  if(record.IsPrimary == &quot;true&quot;)
  {
    contactId = record.Contact.Id;
    contactName = record.Contact.Name;
    contactEmail = record.Contact.Email;
  }
  else
  {
    cc.push(record.Contact.Email);
  }
}
var u = &quot;https://toutapp.com/pitches/new?name=&quot; + contactName + &quot;&amp;email=&quot; + contactEmail + &quot;&amp;force_bookmarklet=true&amp;style=salesforce&amp;salesforce_contact_id=&quot; + contactId + &quot;&amp;salesforce_opportunity_id={!Opportunity.Id}&amp;cc=&quot; + cc.join(&quot;,&quot;); 

var w = window; 

w.open(u,&apos;Tout&apos;,&apos;scrollbars=1,status=1,width=762,height=&apos; + ( w.screen.height &lt; 970 ? w.screen.height : 970));</url>
