        <fullName>Send_For_Signature</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send For Signature</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)} 
{!requireScript(&quot;/soap/ajax/36.0/apex.js&quot;)} 
console.log(&apos;11111111&apos;);
var oppStage = &apos;{!Opportunity.StageName}&apos;;
var recOwnerId = &apos;{!Opportunity.OwnerId}&apos;;

if(oppStage == &apos;Closed Won&apos;) { 
	alert(&apos;{!$Label.Opportunity_Closed_Won_Reopen_Error}&apos;); 
} 
else if(oppStage == &apos;Closed Lost&apos;) { 
	alert(&apos;{!$Label.Opportunity_Closed_Lost_Reopen_Error}&apos;); 
}

// CSP-2045 Non-Owners sending opps for signature, add one more logic to check if the current user is the record owner if profile is AE
else if(recOwnerId !=&apos;{!User.Id}&apos; &amp;&amp; &apos;{!$Profile.Name}&apos; == &apos;Local Core Account Executive&apos;){
	alert(&apos;Only Opportunity Owner can submit the record for signature!&apos;);
} 

else{ 
	var response = sforce.apex.execute(&quot;JavascriptButtonService&quot;,&quot;validateSignatureProceed&quot;,{oppId:&quot;{!Opportunity.Id}&quot;}); 
	response = JSON.parse(response[0]); 

	if(response.success) { 
		window.location = &quot;/apex/EchoSignSendForSignature?masterId={!Opportunity.Id}&amp;templateId=&quot; +response.agreementTemplateId; 
	} 
	else if (response.errorMessage.includes(&apos;Term Start Date of less than 5 business days from today&apos;)) { 
		var proceedAnyway = window.confirm(response.errorMessage); 
		if (proceedAnyway) { 
			var response = sforce.apex.execute(&quot;JavascriptButtonService&quot;,&quot;updateEchosignExpirationDate&quot;,{oppId:&quot;{!Opportunity.Id}&quot;}); 
			response = JSON.parse(response[0]); 
			if (response.success) { 
				window.location = &quot;/apex/EchoSignSendForSignature?masterId={!Opportunity.Id}&amp;templateId=&quot; +response.agreementTemplateId; 
			} 
			else { 
				alert(response.errorMessage); 
			} 
		} 
	} 
	else if(response.errorMessage.includes(&apos;The Primary/Billing Contact must have&apos;)) { 
		var proceedAnyway = window.confirm(response.errorMessage); 
		if(proceedAnyway) { 
			var contactURL = response.errorMessage.substring(response.errorMessage.indexOf(&apos;URL: &apos;) + 5); 
			window.location = contactURL; 
		} 
	} 
	else { 
		alert(response.errorMessage); 
	} 
}

console.log(&apos;11111111222 end&apos;);</url>
