        <fullName>Unlock_Quotes</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Unlock Quotes</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

var oppStage = &apos;{!Opportunity.StageName}&apos;; 
if(oppStage == &apos;Closed Won&apos;) {
	alert(&apos;{!$Label.Opportunity_Closed_Won_Reopen_Error}&apos;); 
}
else if(oppStage == &apos;Closed Lost&apos;) {
	alert(&apos;{!$Label.Opportunity_Closed_Lost_Reopen_Error}&apos;); 
}
else{
	var isLocked = &apos;{!Opportunity.Is_Record_Locked__c}&apos; === &apos;0&apos;;

	if(isLocked)
	{
	if(oppStage != &apos;Closed Won&apos; &amp;&amp; oppStage != &apos;Closed Lost&apos;){ 
	var newRecords = [];
	 
	var c = new sforce.SObject(&quot;Opportunity&quot;); 
	c.id =&quot;{!Opportunity.Id}&quot;;
	c.Lock_Quotes__c = &apos;0&apos;;
	c.StageName = &apos;Quoting&apos;;
	newRecords.push(c); 
	 
	var result = sforce.connection.update(newRecords); 
        
        if (result[0].success==&apos;false&apos;) {
           alert(result[0].errors);
        } else {
           window.location.reload();
        }

	 
	window.location.reload();
	}
	else
	{
	if(oppStage == &apos;Closed Won&apos;) 
	alert(&apos;Opportunity is in Closed Won Stage.&apos;); 
	if(oppStage == &apos;Closed Lost&apos;) 
	alert(&apos;Opportunity is in Closed Lost Stage. Please Create a New Opportunity to Add a New Quote&apos;); 
	}
	}
	else
	{
	alert(&apos;Opportunity is Locked and Pending Internal Approvals&apos;);
	}
}</url>
