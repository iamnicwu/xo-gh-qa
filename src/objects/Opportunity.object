    <validationRules>
        <fullName>Account_is_required</fullName>
        <active>true</active>
        <description>Account is always required</description>
        <errorConditionFormula>ISNULL(AccountId)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Account is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_Close_Date</fullName>
        <active>true</active>
        <description>Actual Close Date can only be changed by system admin</description>
        <errorConditionFormula>ISCHANGED( Actual_Close_Date__c ) &amp;&amp; $Profile.Name = &apos;National Users&apos;</errorConditionFormula>
        <errorDisplayField>Actual_Close_Date__c</errorDisplayField>
        <errorMessage>You do not have permission to change Actual Close Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Agency_Buy_Needs_an_Agency</fullName>
        <active>true</active>
        <description>If Agency Buy is selected for Order Type, an Agency is required</description>
        <errorConditionFormula>NOT(ISPICKVAL(StageName  , &apos;Closed Lost&apos;)) &amp;&amp; ISPICKVAL( Order_Type__c , &apos;Agency Buy&apos;) &amp;&amp;  ISBLANK( Agency__c ) &amp;&amp; RecordType.Name = &apos;National&apos;</errorConditionFormula>
        <errorDisplayField>Agency__c</errorDisplayField>
        <errorMessage>Agency is required if the Opportunity Order Type is Agency Buy</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_Not_Edit_Concierge_Opp</fullName>
        <active>false</active>
        <description>Lock Concierge Opp</description>
        <errorConditionFormula>AND
(
RecordType.Name = &apos;Opp Concierge&apos;
/*,$Profile.Name &lt;&gt; &quot;System Administrator&quot;*/
)</errorConditionFormula>
        <errorDisplayField>No_of_Times_To_Extend__c</errorDisplayField>
        <errorMessage>You can not create or edit the Concierge Contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_not_edit_the_Inventory_Release_Date</fullName>
        <active>true</active>
        <description>The Opportunity Expiration Date can not be updated while the opportunity is in the &quot;Pending Client Approval&quot; and &quot;Client Approved&quot;stage</description>
        <errorConditionFormula>AND($Profile.Name =&apos;Local Core Account Executive&apos;,
ISCHANGED(Opportunity_Expiration_Date__c),OR(ISPICKVAL(StageName, &apos;Pending Client Approval&apos;), ISPICKVAL(StageName, &apos;Client Approved&apos;)))</errorConditionFormula>
        <errorMessage>The Opportunity Expiration Date can not be updated while the opportunity is in the current stage. Please move back to the Quoting stage to adjust this date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Modify_the_Contract_Signer</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name &lt;&gt; &quot;System Administrator&quot;,$Profile.Name &lt;&gt; &quot;Service Accounts&quot; , ISCHANGED( Contract_Signer__c ) , OR(ISPICKVAL(StageName , &quot;Pending Client Approval&quot; ) , ISPICKVAL(StageName , &quot;Client Approved&quot; ) , ISPICKVAL(StageName , &quot;Closed Won&quot; )) , RecordType.DeveloperName = &quot;Local&quot;)</errorConditionFormula>
        <errorDisplayField>Contract_Signer__c</errorDisplayField>
        <errorMessage>You can&apos;t modify Contract Signer after quoting!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_Change_Account_Strategist_to_Null</fullName>
        <active>true</active>
        <description>Can not change Account Strategist value to null</description>
        <errorConditionFormula>ISCHANGED(AccStrategist__c) &amp;&amp; ISBLANK(AccStrategist__c)</errorConditionFormula>
        <errorDisplayField>AccStrategist__c</errorDisplayField>
        <errorMessage>You can not change the Account Strategist field to a blank value!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closing_Validation</fullName>
        <active>true</active>
        <description>Validate that an Opportunity is on Verbally Approved before it can select Closing</description>
        <errorConditionFormula>AND(
  ISPICKVAL(StageName, &apos;Closing&apos;),
  OR(
    AND(
      OR(
        TEXT(PRIORVALUE( StageName )) != &apos;Verbally Approved&apos;,
        AND(
          $User.Id !=  AccStrategist__c,
          $Profile.Name != &apos;System Administrator&apos;
        )
      ),
     ISCHANGED(StageName)
    ),
   ISNEW()
 )
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>You can&apos;t select Closing until an Opportunity has been on Verbally Approved.
Only  Account Strategist can move the Opportunity to Closing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_Stages_new_opportunities</fullName>
        <active>false</active>
        <description>requested on SS-6997</description>
        <errorConditionFormula>AND(
ISNEW(),
 OR(   
ISPICKVAL(StageName,&apos;Qualifying&apos;),
ISPICKVAL(StageName,&apos;Developing&apos;),
ISPICKVAL(StageName,&apos;Quoting&apos;),
ISPICKVAL(StageName,&apos;Closing&apos;),
ISPICKVAL(StageName,&apos;Pending Internal Approval&apos;),
ISPICKVAL(StageName,&apos;Ready for Echo-Sign&apos;),
ISPICKVAL(StageName,&apos;Pending Client Approval&apos;),
ISPICKVAL(StageName,&apos;Client Approved&apos;),
ISPICKVAL(StageName,&apos;Closed Won&apos;),
ISPICKVAL(StageName,&apos;Closed Lost&apos;),
ISPICKVAL(StageName,&apos;Calculating TCV&apos;),
ISPICKVAL(StageName,&apos;Customer Declined&apos;),
ISPICKVAL(StageName,&apos;No Customer Response&apos;),
ISPICKVAL(StageName,&apos;Negotiating&apos;),
ISPICKVAL(StageName,&apos;Verbally Approved&apos;)
),

RecordType.DeveloperName = &apos;National&apos;
)</errorConditionFormula>
        <errorMessage>New National Opportunities need to have a Stage of; Prospecting, Value Proposition, or Pitched</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_Stages_new_opportunities_V2</fullName>
        <active>true</active>
        <description>requested on SS-6997</description>
        <errorConditionFormula>AND(
ISNEW(),
AND(   
NOT ISPICKVAL(StageName,&apos;Prospecting&apos;),
NOT ISPICKVAL(StageName,&apos;Value Proposition&apos;),
NOT ISPICKVAL(StageName,&apos;Pitched&apos;)
),

RecordType.DeveloperName = &apos;National&apos;
)</errorConditionFormula>
        <errorMessage>New National Opportunities need to have a Stage of; Prospecting, Value Proposition, or Pitched</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Reason_Details_Required</fullName>
        <active>true</active>
        <description>for ticket CSP-923,</description>
        <errorConditionFormula>AND( 
/* only new opportunities or when they are being closed 
do not throw an error when updating historic records */ 
OR( 
ISNEW(), 
ISCHANGED(StageName) 
), 
/* only closed lost opps */ 
IsClosed, 
NOT(IsWon), 
/* where the Close lost Noteis not filled in */ 
 ISBLANK( Lost_Reason_Details__c ) ,
RecordType.Name = &apos;National&apos;
)</errorConditionFormula>
        <errorDisplayField>Lost_Reason_Details__c</errorDisplayField>
        <errorMessage>A Lost Reason Detail must be filled when an Opportunity is Lost</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Reason_Required</fullName>
        <active>true</active>
        <description>When marking an Opportunity as Closed Lost, the Lost Reason is required</description>
        <errorConditionFormula>AND(
    /* only new opportunities or when they are being closed
       do not throw an error when updating historic records */
    OR(
       ISNEW(),
       ISCHANGED(StageName)
      ),
    /* only closed lost opps */
    IsClosed, 
    NOT(IsWon), 
    /* where the lost reason is not filled in */
    ISPICKVAL(Lost_Reason__c, &quot;&quot;)
/*,Comment for ticket CSP-923
     RecordType.Name != &apos;National&apos;*/
   )</errorConditionFormula>
        <errorDisplayField>Lost_Reason__c</errorDisplayField>
        <errorMessage>A Lost Reason must be selected when an Opportunity is Lost</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Reason_not_needed_for_Won_Opps</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
    IsWon, 
    OR(
       NOT(IsPickVal(Lost_Reason__c,&quot;&quot;)),
       NOT(IsBlank(Lost_Reason_Details__c))
      )
    )</errorConditionFormula>
        <errorDisplayField>Lost_Reason__c</errorDisplayField>
        <errorMessage>Do not enter a value for Lost Reason or Lost Reason Details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_filled_Contract_Signer_beforeQuote</fullName>
