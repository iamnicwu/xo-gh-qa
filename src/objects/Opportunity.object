			var retVal = sforce.apex.execute(&apos;JavascriptButtonService&apos;, &apos;updateAutoCloseDate&apos;, {oppId: &quot;{!Opportunity.Id}&quot;});

			console.log(retVal[0]); 
			response = JSON.parse(retVal[0]); 
			console.log(retVal.errorMessage); 

