    <validationRules>
        <fullName>Can_Not_Edit_Concierge_Contract</fullName>
        <active>false</active>
        <description>Lock Concierge Opp Contract</description>
        <errorConditionFormula>And( 
Opportunity__r.RecordType.Name = &apos;Opp Concierge&apos;
/*,$Profile.Name &lt;&gt; &quot;System Administrator&quot; */
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>You can not create or edit the Concierge Contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_company_signed_by</fullName>
        <active>true</active>
        <description>When the contract is signed, require the company signed by</description>
        <errorConditionFormula>AND(
    OR(ISNEW(),ISCHANGED(Status)),
    ISPICKVAL(Status, &apos;Signed&apos;),
    ISBLANK( CompanySignedBy__c )
   )</errorConditionFormula>
        <errorDisplayField>CompanySignedBy__c</errorDisplayField>
        <errorMessage>Company Signed By is required when marking a contract Signed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_company_signed_datetime</fullName>
        <active>true</active>
        <description>When the contract is signed, require the company signed datetime</description>
        <errorConditionFormula>AND(
    OR(ISNEW(),ISCHANGED(Status)),
    ISPICKVAL(Status, &apos;Signed&apos;),
    ISBLANK( CompanySignedDateTime__c )
   )</errorConditionFormula>
        <errorDisplayField>CompanySignedDateTime__c</errorDisplayField>
        <errorMessage>Company Signed Date/Time is required when marking a contract Signed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_customer_signed_by</fullName>
        <active>true</active>
        <description>When the contract is signed, require the customer sign date</description>
        <errorConditionFormula>AND(
    OR(ISNEW(),ISCHANGED(Status)),
    ISPICKVAL(Status, &apos;Signed&apos;),
    ISBLANK(CustomerSignedBy__c)
   )</errorConditionFormula>
        <errorDisplayField>CustomerSignedBy__c</errorDisplayField>
        <errorMessage>Customer Signed By is required when marking a contract Signed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_customer_signed_datetime</fullName>
        <active>true</active>
        <description>When the contract is signed, require the customer sign date</description>
        <errorConditionFormula>AND(
    OR(ISNEW(),ISCHANGED(Status)),
    ISPICKVAL(Status, &apos;Signed&apos;),
    ISBLANK( CustomerSignedDateTime__c)
   )</errorConditionFormula>
        <errorDisplayField>CustomerSignedDateTime__c</errorDisplayField>
        <errorMessage>Customer Signed Date/Time is required when marking a contract Signed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_payment_method_by_Statu</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Preferred_Payment_Method__c , &apos;&apos;) , ISPICKVAL( Status , &apos;Signed&apos;) )</errorConditionFormula>
        <errorDisplayField>Preferred_Payment_Method__c</errorDisplayField>
        <errorMessage>Preferred Payment Method field is required when the [Status] value on a Contract record is set to &quot;Signed&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_ContractFeePerReq</fullName>
        <active>true</active>
        <description>Phase 1 - either Contract % or Contract Fee is required.</description>
        <errorConditionFormula>AND( ISNULL( Contract__c ),  ISNULL( ContractFee__c ) )</errorConditionFormula>
        <errorMessage>Please capture either Contract % or Contract Fee.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_FlatFeePer</fullName>
        <active>true</active>
        <description>Phase 1 - If Payment Terms is Flat Fee, Contract % field must be Null.</description>
        <errorConditionFormula>AND ( (ISPICKVAL( PaymentTerm__c , &apos;Flat Fee $&apos;)),  NOT(ISNULL(  Contract__c )))</errorConditionFormula>
        <errorMessage>Payment Terms is Flat Fee $, please capture Contract Fee.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_PercContFee</fullName>
        <active>true</active>
        <description>Phase 1 - If Payment Terms is Percentage, Contract Fee field must be Null.</description>
        <errorConditionFormula>AND ( (ISPICKVAL( PaymentTerm__c , &apos;Percentage&apos;)),  NOT(ISNULL(  ContractFee__c )))</errorConditionFormula>
        <errorMessage>Payment Terms is Percentage, please capture Contract %.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>New_Contract</fullName>
        <availability>online</availability>
        <description>Replaces the New Contract button on the Opportunity layout to include the Account in the link.</description>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>New Contract</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>/800/e?CF00Nj0000008b5Y5={!Opportunity.Name}&amp;CF00Nj0000008b5Y5_lkid={!Opportunity.Id}&amp;retURL=%2F{!Opportunity.Id}&amp;accid={!Opportunity.AccountId}</url>
    </webLinks>
    <webLinks>
        <fullName>SendEchosign</fullName>
        <availability>online</availability>
        <description>Phase 1</description>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Send to Echosign</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/echosign_dev1__AgreementTemplateProcess?masterId={!Contract.Id}&amp;templateId={!IF(ISPICKVAL(Contract.PaymentTerm__c,&apos;Flat Fee $&apos;), &quot;a03j000000L7maC&quot;,&quot;a03j000000L7fKn&quot;)}</url>
    </webLinks>
    <webLinks>
        <fullName>echosign_dev1__Send_to_EchoSign</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Send for Signature</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/echosign_dev1__AgreementTemplateProcess?masterId={!Contract.Id}</url>
    </webLinks>
</CustomObject>
