    <validationRules>
        <fullName>Account_Strategist_Required_in_Opp</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Opportunity__r.AccStrategist__c )</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Related Opportunity must have an Account Strategist!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campaign_Start_Date_and_End_Date</fullName>
        <active>true</active>
        <description>Campaign Start Date must be before Campaign End Date</description>
        <errorConditionFormula>Campaign_Start_Date__c &gt; Campaign_End_Date__c</errorConditionFormula>
        <errorDisplayField>Campaign_End_Date__c</errorDisplayField>
        <errorMessage>Campaign Start Date can&apos;t be after Campaign End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Internal_Due_Date_validation</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Internal_Due_Date__c ) &amp;&amp; ( Internal_Due_Date__c &lt;= TODAY() + 1)</errorConditionFormula>
        <errorDisplayField>Internal_Due_Date__c</errorDisplayField>
        <errorMessage>Cannot choose a date within 24 hrs</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_must_have_product</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Name == &apos;RFP&apos; &amp;&amp; ISBLANK(Opportunity__r.Pricebook2Id)</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Related Opportunity must have a pricebook selected before an RFP can be created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Please_fill_in_a_Success_Metric</fullName>
        <active>true</active>
        <description>Success Metric Detail Validation</description>
        <errorConditionFormula>OR(
AND(Clicks__c, ISBLANK( CTR__c ), ISBLANK(Cost_Per_Click__c)), 
AND(End_Actions__c, ISBLANK(No_of_Sales__c), ISBLANK(Total_Sales__c), ISBLANK(No_of_Actions__c), ISBLANK(Cost_Per_Action__c), ISBLANK( Action_Explanation__c )),
AND(Leads__c, ISBLANK(No_of_Op_ins__c), ISBLANK(Cost_Per_Lead__c)),
AND(Brand_Exposure__c, ISBLANK(Other__c), ISBLANK(Marketing_Channel__c))
)</errorConditionFormula>
        <errorMessage>Please fill in the details for Clicks, End Actions, Leads or Brand Exposure Success Metric, if the relating checkbox is selected!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Dev_Needed_for_Custom_Content</fullName>
        <active>true</active>
        <errorConditionFormula>PRIORVALUE(Sales_Dev_Needed__c) &amp;&amp; NOT(Sales_Dev_Needed__c) &amp;&amp; Custom_Content__c</errorConditionFormula>
        <errorDisplayField>Sales_Dev_Needed__c</errorDisplayField>
        <errorMessage>Sales Dev Needed is required if an RFP needs Custom Content</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Success_Metric_Required</fullName>
        <active>true</active>
        <description>Must fill in one of the Success Metric section</description>
        <errorConditionFormula>AND(NOT(Clicks__c), NOT(End_Actions__c), NOT(Leads__c), NOT(Brand_Exposure__c))</errorConditionFormula>
        <errorMessage>Please fill in a Success Metric to give Account Strategist more details!</errorMessage>
    </validationRules>
