        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>// Establish a connection to the API,
// using the current session ID
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;;
 
var ts = new sforce.SObject(&quot;ThoughtStarter_RFP__c&quot;);
ts.Id = &quot;{!ThoughtStarter_RFP__c.Id}&quot;;
ts.Submitted_for_Production__c = true;
 
var results = sforce.connection.update([ts]);
 
// Examine the results to see whether our update was successful.
 
if (results[0].getBoolean(&quot;success&quot;)) {
    location.reload();
} else {
    /*var errorMsgValue = &apos;{!ThoughtStarter_RFP__c.RecordType} could not be submitted!\nPlease review the following errors:\n&apos; + results[0].errors.message.replace(/&lt;br\s*\/?&gt;/mg,&quot;\n&quot;);*/
    alert(results[0].errors.message.replace(/&lt;br\s*\/?&gt;/mg,&quot;\n&quot;));
}</url>
